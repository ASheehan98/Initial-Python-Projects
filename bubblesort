a = [5,3,2,6,7,8,1,9,10,12]
b = [6,5,4,3,2,1]

def bubblesort(input):
    n = len(input)

    for i in range(n):

        for j in range(0, n - 1 - i):

            if input[j] > input[j + 1]:
                input[j], input[j+1]= input[j+1], input[j]
    return(input)
print(bubblesort(a))
print(bubblesort(b))
